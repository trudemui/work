        "v-scroll-lock": "^1.3.1",
        "vee-validate": "^3.4.10",
        "vue-js-modal": "^2.0.1",
        "vue-loading-template": "^1.3.2",
        "vue-router": "^3.5.1",
        "vue-select": "^3.10.8",
        "vuejs-datepicker": "^1.6.2",
        "vuejs-paginate": "^2.1.0",
        "vuex": "^3.6.2"
    }
